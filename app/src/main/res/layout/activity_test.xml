<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".TestActivity">



    <Button
        android:id="@+id/activity_main_close_uninstall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭禁止卸载" />
    <Button
        android:id="@+id/activity_main_close_update_system"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭系统更新" />

    <Button
        android:id="@+id/activity_main_close_factory_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="关闭禁止恢复出厂设置" />

    <Button
        android:id="@+id/activity_main_start_app_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="开启应用搜索" />
    <Button
        android:id="@+id/activity_main_remove_position_app"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="移除激活" />
</LinearLayout>